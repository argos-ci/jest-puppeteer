    "build": "rollup -c"
  },
  "devDependencies": {
    "puppeteer": "^23.11.1",
    "rollup": "^4.29.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-swc3": "^0.12.1"
  }
