{
  "name": "jest-puppeteer",
  "description": "Run your tests using Jest & Puppeteer.",
  "version": "7.0.1",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/globals.d.ts"
    },
    "./globals": {
      "default": "./dist/globals.js",
      "types": "./dist/globals.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/argos-ci/jest-puppeteer.git",
    "directory": "packages/jest-puppeteer"
  },
  "homepage": "https://github.com/argos-ci/jest-puppeteer/tree/main/packages/jest-puppeteer#readme",
  "bugs": {
    "url": "https://github.com/argos-ci/jest-puppeteer/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "jest",
    "puppeteer",
    "jest-puppeteer",
    "chromeless",
    "chrome-headless"
  ],
  "peerDependencies": {
    "puppeteer": ">=19"
  },
  "dependencies": {
    "expect-puppeteer": "^7.0.1",
    "jest-environment-puppeteer": "^7.0.1"
  },
  "devDependencies": {
    "rollup": "^3.15.0",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-swc3": "^0.8.0"
  }
}
