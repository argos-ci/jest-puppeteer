{
  "name": "jest-environment-puppeteer",
  "description": "Puppeteer environment for Jest.",
  "version": "10.0.0",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./setup": "./setup.js",
    "./teardown": "./teardown.js",
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/argos-ci/jest-puppeteer.git",
    "directory": "packages/jest-environment-puppeteer"
  },
  "homepage": "https://github.com/argos-ci/jest-puppeteer/tree/main/packages/jest-environment-puppeteer#readme",
  "bugs": {
    "url": "https://github.com/argos-ci/jest-puppeteer/issues"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "jest",
    "jest-environment",
    "puppeteer",
    "jest-puppeteer",
    "chromeless",
    "chrome-headless"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "rollup -c"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@types/jest": "^29.5.9",
    "rollup": "^4.5.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-swc3": "^0.10.4"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "cosmiconfig": "^8.3.6",
    "deepmerge": "^4.3.1",
    "jest-dev-server": "^10.0.0",
    "jest-environment-node": "^29.7.0"
