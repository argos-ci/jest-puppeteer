{
  "name": "jest-environment-puppeteer",
  "description": "Puppeteer environment for Jest.",
  "version": "7.0.1",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./globals": {
      "types": "./dist/globals.d.ts",
      "default": "./dist/globals.js"
    },
    "./setup": "./setup.js",
    "./teardown": "./teardown.js",
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/argos-ci/jest-puppeteer.git",
    "directory": "packages/jest-environment-puppeteer"
  },
  "homepage": "https://github.com/argos-ci/jest-puppeteer/tree/main/packages/jest-environme#readme",
  "bugs": {
    "url": "https://github.com/argos-ci/jest-puppeteer/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "jest",
    "jest-environment",
    "puppeteer",
    "jest-puppeteer",
    "chromeless",
    "chrome-headless"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "rollup -c"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@jest/globals": "29.4.3",
    "rollup": "^3.15.0",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-swc3": "^0.8.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "cosmiconfig": "^8.0.0",
    "jest-dev-server": "^7.0.1",
    "jest-environment-node": "^29.4.3",
    "deepmerge": "^4.3.0"
  }
}
